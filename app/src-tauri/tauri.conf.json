{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "althand",
  "version": "0.1.0",
  "identifier": "com.althand.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "url": "http://localhost:1420",
        "fullscreen": false,
        "height": 250,
        "width": 800,
        "resizable": false,
        "decorations": false,
        "center": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}